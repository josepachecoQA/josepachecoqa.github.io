  "repository": {
    "type": "git",
    "url": "https://github.com/seu-usuario/pagina-profissional"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.12.7",
    "istanbul-lib-instrument": "^6.0.3",
    "nyc": "^15.1.0"
  }
}
