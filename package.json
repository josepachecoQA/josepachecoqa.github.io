{
  "name": "jose-pacheco-qa-engineer",
  "version": "1.0.0",
  "description": "Landing page profissional de QA Engineer Pleno - Qualidade orientada ao negócio",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "build": "echo 'Build completo. Pronto para publicação.'",
    "deploy": "echo 'Use GitHub Pages, Vercel ou Netlify para deploy'",
    "test": "npx cypress run",
    "test:open": "npx cypress open",
    "test:headed": "npx cypress run --headed",
    "test:debug": "npx cypress run --debug",
    "coverage:prepare": "node scripts/prepare-coverage.js",
    "coverage:report": "nyc report --reporter=json-summary --report-dir reports/coverage"
  },
  "keywords": [
    "qa",
    "quality-assurance",
    "testing",
    "portfólio",
    "landing-page"
  ],
